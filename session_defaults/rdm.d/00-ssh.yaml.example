#
#  This is example config file
#

# Default session settings
# Connection type #77: SSH


ssh:
  excel:
    port: "22"    
    rdm_credential: "example/my rdm credential"
    rdm_host: "example/my rdm credential"
    rdm_script_before_open: "%HOMEPATH%\\win\\path\\my-script.ps1 -argument value"
  raw:
    ConnectionType: 77    
    AllowPasswordVariable: true
    CredentialPrivateVaultSearchString: "my\\users-vault"
    Terminal: 
      Host: "10.10.10.20"
      HostPort: "22"
      Username: "username"            
    Events:
      - BeforeConnectionEmbeddedPowerShellScript: "%HOMEPATH%\\win\\path\\my-script.ps1 -argument value"